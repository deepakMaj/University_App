<div class="container">
  <div class="row">
    <div class="col s12">
      <div class="card-panel red lighten-2">
        <% if obj.errors.any? %>
          <strong><%= pluralize(obj.errors.count, "error")%>
          prevented course from being saved :</strong>
          <ul>
            <%= obj.errors.full_messages.each do |msg| %>
              <li><%= msg %></li>
            <% end %>
          </ul>
      </div>
    </div>
  </div>
</div>
